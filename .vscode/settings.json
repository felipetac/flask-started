{
    "python.pythonPath": "${workspaceFolder}/.venv/bin/python",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.pyc": {"when": "$(basename).py"}, 
        "**/__pycache__": true
    },
    "python.linting.enabled": true,
    
}